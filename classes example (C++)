#include<iostream>
using namespace std;

class dondon {
private:
string name;
int health;
int strength;
int def;
//int takeDam;
public:
dondon();
dondon(int h, int s, int d, string n);
void printinfo();
};

int main() {
dondon gary(100, 50, 50, "gary");
gary.printinfo();
gary.printinfo();
}

dondon::dondon(int h, int s, int d, string n) {
health = h;
strength = s;
def = d;
name = n;
}

//void dondon::takeDam(int damage,int health, int def) {

//}
void dondon::printinfo() {
cout << strength << endl;
}
